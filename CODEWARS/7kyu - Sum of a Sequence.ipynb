    "def sequence_sum(begin_number, end_number, step):\n",
    "    number = 0\n",
    "    my_list=list(range(begin_number,end_number))\n",
    "    for i in my_list:\n",
    "        number = number + i*step\n",
    "    return number"
   ]
  },
